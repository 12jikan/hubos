    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.15",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "prepare-commit-msg": "./.tools/git-commit-message.sh $HUSKY_GIT_PARAMS"
